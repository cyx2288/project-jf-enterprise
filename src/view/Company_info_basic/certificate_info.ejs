                    <h4 class="modal-title" id="staff_lookup_editLabel">营业执照模板 </h4>
                </div>
                <!-- body-->
                <div class="modal-body">
                    <div class="business_license"><img src="../../icon/company_photo.jpg"></div>
                    <!-- footer -->
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary btn-primary-zy model-change1 edit_close" data-dismiss="modal">确认</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--企业认证模板-->
    <div class="modal fade" tabindex="-1" aria-labelledby="staff_lookup_editLabel" id="certificate_authority" aria-hidden="true" data-backdrop="static">
